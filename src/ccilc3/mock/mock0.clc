
def maxn : nat -> nat -> nat
  | x y =>
    match x, y with
    | zero, _ => y
    | _, zero => x
    | succ x, succ y => succ (maxn x y)

def one := 
  if true then
    1
  else
    0

def two := 2

def foo := maxn one

def bar := foo two

